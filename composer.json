{
    "name": "jbuncle/simple-dic",
    "description": "Simple Dependency Injection container focussing on auto injection.",
    "type": "library",
    "authors": [
        {
            "name": "James Buncle",
            "email": "jbuncle@hotmail.com"
        }
    ],
    "repositories": [
        {
            "url": "https://nexus.jbuncle.co.uk/repository/composer-private/",
            "type": "composer"
        }
    ],
    "autoload": {
        "psr-4": {
            "SimpleDic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleDic\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "phpunit/php-code-coverage": "^7.0",
        "jbuncle/php-standards": "@stable"
    },
    "require": {
        "php": ">=7.1",
        "psr/container": "^1.0"
    }
}
